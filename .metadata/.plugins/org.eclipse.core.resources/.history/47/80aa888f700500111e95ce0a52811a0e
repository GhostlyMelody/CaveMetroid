import model.Sprite;
import model.entities.Entity;

public class Frame {

	private Image screen;
	
	public Frame() {
		screen = new BufferedImage (Constants.WINDOW_WIDTH, Constants.WINDOW_HEIGHT, BufferedImage.TYPE_INT_ARGB);
	}
	
	public void addSprite(Sprite s) {
		g = screen.getGraphics();
	}

}
