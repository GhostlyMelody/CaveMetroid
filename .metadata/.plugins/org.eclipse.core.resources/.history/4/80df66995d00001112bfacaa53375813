package model;

import controller.GameController;

public class GameModel implements Runnable {

	GameController controller;
	
	public void run() {
		long i = 0;
		public void run() {
			while(true) {
				i++;
				if (i % 10 == 0)
					System.out.println(i);
	}

	public void pause() {
		
	}

	public void setController(GameController gameController) {
		controller = gameController;
	}
}
